{
  "name": "eslint-formatter-autolinkable-stylish",
  "version": "1.0.3",
  "description": "ESlint Autolinkable Stylish Formatter",
  "keywords": [
    "eslint",
    "eslint-formatter",
    "eslint-formatter-autolinkable-stylish",
    "typescript"
  ],
  "author": {
    "name": "Alexander Tarasyuk",
    "email": "alexander.tarasyuk@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-tarasyuk/eslint-formatter-autolinkable-stylish.git"
  },
  "bugs": {
    "url": "https://github.com/a-tarasyuk/eslint-formatter-autolinkable-stylish/issues"
  },
  "main": "dist/autolinkable-stylish-formatter.js",
  "scripts": {
    "prepare": "npm run build",
    "format": "prettier --write \"./**/*.{ts,md}\"",
    "format:check": "prettier --list-different \"./**/*.{ts,md}\"",
    "test": "jest",
    "lint": "eslint -c .eslintrc . --ext .ts",
    "build": "tsc"
  },
  "dependencies": {
    "chalk": "^2.4.2"
  },
  "peerDependencies": {
    "eslint": "^6.5.1"
  },
  "devDependencies": {
    "@types/eslint": "6.1.3",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.6",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "eslint": "5.16.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format:check && npm test"
    }
  }
}
